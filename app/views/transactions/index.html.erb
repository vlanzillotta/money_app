

<%=@current_user.email %>
Here are your transactions!
<table class="table table-striped table-bordered table-condensed">
	<tr>
		<th>Date</th>
		<th>Transaction</th>
		<th>Amount</th>
		<th>Type</th>
	</tr>
<% @transaction.each do |transaction| %>
	<tr class="<%=transaction.type_of%>">
		<td>
			<%= transaction.transaction_date %>
		</td>
		<td>
			<%= transaction.name %>
		</td>
		<td>
			<%= transaction.amount %>
		</td>
		<td>
			<%= transaction.type_of %>
		</td>
	</tr>
<% end %>
</table>
